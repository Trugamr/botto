{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--no-warnings", "--loader", "ts-node/esm", "--watch"],
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/src/index.ts",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    }
  ]
}
